#!/usr/bin/env /usr/bin/bash
export CARGO_NET_GIT_FETCH_WITH_CLI=true
export SSL_CERT_FILE=/var/cache/ca-certs/anchors/ca-certificates.crt
export CARGO_HTTP_CAINFO=/var/cache/ca-certs/anchors/ca-certificates.crt
export CC=clang
export CXX=clang++
export CC_x86_64_unknown_linux_gnu=clang
export CXX_x86_64_unknown_linux_gnu=clang++
export AR=/usr/bin/llvm-ar
export RANLIB=/usr/bin/llvm-ranlib
export NM=/usr/bin/llvm-nm
export AR_x86_64_unknown_linux_gnu=/usr/bin/llvm-ar
export RANLIB_x86_64_unknown_linux_gnu=/usr/bin/llvm-ranlib
export NM_x86_64_unknown_linux_gnu=/usr/bin/llvm-nm
# export CRATE_CC_NO_DEFAULTS=1
export CARGO_BUILD_JOBS=16
export PCRE2_SYS_STATIC=1
export OPENSSL_STATIC=yes
export OPENSSL_LIB_DIR=/usr/lib64/
export OPENSSL_INCLUDE_DIR=/usr/include/
# export LLVM_PROFILE_FILE="/var/tmp/pgo/code-%p-%m.profraw"
unset LLVM_PROFILE_FILE
##
export PGO_GEN="-fprofile-generate=/var/tmp/pgo -fprofile-update=atomic -fPIC"
export CFLAGS_GENERATE="--save-temps -Wl,--save-temps -flto=full -fuse-ld=lld -falign-functions=32 -fno-semantic-interposition -Wl,-Bsymbolic-functions -fno-stack-protector -fno-trapping-math -fno-math-errno -ftree-slp-vectorize -ftree-vectorize -feliminate-unused-debug-types -fno-plt $PGO_GEN"
export CXXFLAGS_GENERATE="--save-temps -Wl,--save-temps -flto=full -fuse-ld=lld -falign-functions=32 -fno-semantic-interposition -Wl,-Bsymbolic-functions -fno-stack-protector -fno-trapping-math -fno-math-errno -ftree-slp-vectorize -ftree-vectorize -feliminate-unused-debug-types -fno-plt $PGO_GEN"
export LDFLAGS_GENERATE="--save-temps -Wl,--save-temps -flto=full -fuse-ld=lld -falign-functions=32 -fno-semantic-interposition -Wl,-Bsymbolic-functions -fno-stack-protector -fno-trapping-math -fno-math-errno -ftree-slp-vectorize -ftree-vectorize -feliminate-unused-debug-types -fno-plt $PGO_GEN"
#
export PGO_GEN_RUST="-Cprofile-generate=/var/tmp/pgo -Clink-arg=-fprofile-generate=/var/tmp/pgo -Clink-arg=-fprofile-update=atomic -Clink-arg=--save-temps -Clink-arg=-Wl,--save-temps -Clink-arg=-fPIC"
# export PGO_GEN_RUST="-Cprofile-generate=/var/tmp/pgo -Clink-arg=-fprofile-update=atomic"
export CARGO_TARGET_X86_64_UNKNOWN_LINUX_GNU_RUSTFLAGS_GENERATE="-Clinker-plugin-lto=on -Clto=fat --emit=dep-info,link,llvm-ir -Csave-temps=yes -Zunstable-options -Zcombine-cgu=yes -Ccodegen-units=1 -Clinker=clang -Ctarget-feature=+crt-static -Cembed-bitcode=yes -Cpanic=abort -Clink-arg=-fuse-ld=lld -Clink-arg=-flto -Crelocation-model=static -Cincremental=false -Cforce-frame-pointers=off -Cforce-unwind-tables=off -Cpanic=abort -Clinker=clang -Clink-arg=-Wno-unused-command-line-argument -Clink-arg=-Wall -Clink-arg=-flto -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-O2 -Clink-arg=-O3 -Clink-arg=-mno-vzeroupper -Clink-arg=-march=native -Clink-arg=-mtune=native -Clink-arg=-fuse-ld=lld -Clink-arg=-Wl,--as-needed -Clink-arg=-Wl,--build-id=sha1 -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-mllvm,-x86-use-vzeroupper=0 -Clink-arg=-Wl,-z,now,-z,relro,-z,max-page-size=0x1000,-z,separate-code -Clink-arg=-Wno-error -Clink-arg=-mprefer-vector-width=256 -Clink-arg=-falign-functions=32 -Clink-arg=-fno-semantic-interposition -Clink-arg=-Wl,-Bsymbolic-functions -Cprefer-dynamic=no -Zplt=off $PGO_GEN_RUST"
export RUSTFLAGS_GENERATE="-Clinker-plugin-lto=on -Clto=fat --emit=dep-info,link,llvm-ir -Csave-temps=yes -Zunstable-options -Zcombine-cgu=yes -Ccodegen-units=1 -Clinker=clang -Ctarget-feature=+crt-static -Cembed-bitcode=yes -Cpanic=abort -Clink-arg=-fuse-ld=lld -Clink-arg=-flto -Crelocation-model=static -Cincremental=false -Cforce-frame-pointers=off -Cforce-unwind-tables=off -Cpanic=abort -Clinker=clang -Clink-arg=-Wno-unused-command-line-argument -Clink-arg=-Wall -Clink-arg=-flto -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-O2 -Clink-arg=-O3 -Clink-arg=-mno-vzeroupper -Clink-arg=-march=native -Clink-arg=-mtune=native -Clink-arg=-fuse-ld=lld -Clink-arg=-Wl,--as-needed -Clink-arg=-Wl,--build-id=sha1 -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-mllvm,-x86-use-vzeroupper=0 -Clink-arg=-Wl,-z,now,-z,relro,-z,max-page-size=0x1000,-z,separate-code -Clink-arg=-Wno-error -Clink-arg=-mprefer-vector-width=256 -Clink-arg=-falign-functions=32 -Clink-arg=-fno-semantic-interposition -Clink-arg=-Wl,-Bsymbolic-functions -Cprefer-dynamic=no -Zplt=off $PGO_GEN_RUST"
export CARGO_HOST_RUSTFLAGS_GENERATE="-Clinker-plugin-lto=on -Clto=fat --emit=dep-info,link,llvm-ir -Csave-temps=yes -Zunstable-options -Zcombine-cgu=yes -Ccodegen-units=1 -Clinker=clang -Ctarget-feature=+crt-static -Cembed-bitcode=yes -Cpanic=abort -Clink-arg=-fuse-ld=lld -Clink-arg=-flto -Crelocation-model=static -Cincremental=false -Cforce-frame-pointers=off -Cforce-unwind-tables=off -Cpanic=abort -Clinker=clang -Clink-arg=-Wno-unused-command-line-argument -Clink-arg=-Wall -Clink-arg=-flto -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-O2 -Clink-arg=-O3 -Clink-arg=-mno-vzeroupper -Clink-arg=-march=native -Clink-arg=-mtune=native -Clink-arg=-fuse-ld=lld -Clink-arg=-Wl,--as-needed -Clink-arg=-Wl,--build-id=sha1 -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-mllvm,-x86-use-vzeroupper=0 -Clink-arg=-Wl,-z,now,-z,relro,-z,max-page-size=0x1000,-z,separate-code -Clink-arg=-Wno-error -Clink-arg=-mprefer-vector-width=256 -Clink-arg=-falign-functions=32 -Clink-arg=-fno-semantic-interposition -Clink-arg=-Wl,-Bsymbolic-functions -Cprefer-dynamic=no -Zplt=off $PGO_GEN_RUST"
##
export PGO_USE="-fprofile-use=/var/tmp/pgo/code.profdata -fprofile-update=atomic -Wl,--lto-pgo-warn-mismatch -mllvm -pgo-warn-missing-function -Wbackend-plugin -fPIC"
export CFLAGS_USE="--save-temps -Wl,--save-temps -flto=full -fuse-ld=lld -falign-functions=32 -fno-semantic-interposition -Wl,-Bsymbolic-functions -fno-stack-protector -fno-trapping-math -fno-math-errno -ftree-slp-vectorize -ftree-vectorize -feliminate-unused-debug-types -fno-plt $PGO_USE"
export CXXFLAGS_USE="--save-temps -Wl,--save-temps -flto=full -fuse-ld=lld -falign-functions=32 -fno-semantic-interposition -Wl,-Bsymbolic-functions -fno-stack-protector -fno-trapping-math -fno-math-errno -ftree-slp-vectorize -ftree-vectorize -feliminate-unused-debug-types -fno-plt $PGO_USE"
export LDFLAGS_USE="--save-temps -Wl,--save-temps -flto=full -fuse-ld=lld -falign-functions=32 -fno-semantic-interposition -Wl,-Bsymbolic-functions -fno-stack-protector -fno-trapping-math -fno-math-errno -ftree-slp-vectorize -ftree-vectorize -feliminate-unused-debug-types -fno-plt $PGO_USE"
#
export PGO_USE_RUST="-Cprofile-use=/var/tmp/pgo/code.profdata -Clink-arg=-fprofile-use=/var/tmp/pgo/code.profdata -Clink-arg=-fprofile-update=atomic -Cllvm-args=-pgo-warn-missing-function -Clink-arg=-Wl,--lto-pgo-warn-mismatch -Clink-arg=--save-temps -Clink-arg=-Wl,--save-temps -Clink-arg=-fPIC"
# export PGO_USE_RUST="-Cprofile-use=/var/tmp/pgo/code.profdata -Clink-arg=-fprofile-update=atomic -Cllvm-args=-pgo-warn-missing-function -Clink-arg=-Wl,--lto-pgo-warn-mismatch"
export CARGO_TARGET_X86_64_UNKNOWN_LINUX_GNU_RUSTFLAGS_USE="-Clinker-plugin-lto=on -Clto=fat --emit=dep-info,link,llvm-ir -Csave-temps=yes -Zunstable-options -Zcombine-cgu=yes -Ccodegen-units=1 -Clinker=clang -Ctarget-feature=+crt-static -Cembed-bitcode=yes -Cpanic=abort -Clink-arg=-fuse-ld=lld -Clink-arg=-flto -Crelocation-model=static -Cincremental=false -Cforce-frame-pointers=off -Cforce-unwind-tables=off -Cpanic=abort -Clinker=clang -Clink-arg=-Wno-unused-command-line-argument -Clink-arg=-Wall -Clink-arg=-flto -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-O2 -Clink-arg=-O3 -Clink-arg=-mno-vzeroupper -Clink-arg=-march=native -Clink-arg=-mtune=native -Clink-arg=-fuse-ld=lld -Clink-arg=-Wl,--as-needed -Clink-arg=-Wl,--build-id=sha1 -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-mllvm,-x86-use-vzeroupper=0 -Clink-arg=-Wl,-z,now,-z,relro,-z,max-page-size=0x1000,-z,separate-code -Clink-arg=-Wno-error -Clink-arg=-mprefer-vector-width=256 -Clink-arg=-falign-functions=32 -Clink-arg=-fno-semantic-interposition -Clink-arg=-Wl,-Bsymbolic-functions -Cprefer-dynamic=no -Zplt=off $PGO_USE_RUST"
export RUSTFLAGS_USE="-Clinker-plugin-lto=on -Clto=fat --emit=dep-info,link,llvm-ir -Csave-temps=yes -Zunstable-options -Zcombine-cgu=yes -Ccodegen-units=1 -Clinker=clang -Ctarget-feature=+crt-static -Cembed-bitcode=yes -Cpanic=abort -Clink-arg=-fuse-ld=lld -Clink-arg=-flto -Crelocation-model=static -Cincremental=false -Cforce-frame-pointers=off -Cforce-unwind-tables=off -Cpanic=abort -Clinker=clang -Clink-arg=-Wno-unused-command-line-argument -Clink-arg=-Wall -Clink-arg=-flto -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-O2 -Clink-arg=-O3 -Clink-arg=-mno-vzeroupper -Clink-arg=-march=native -Clink-arg=-mtune=native -Clink-arg=-fuse-ld=lld -Clink-arg=-Wl,--as-needed -Clink-arg=-Wl,--build-id=sha1 -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-mllvm,-x86-use-vzeroupper=0 -Clink-arg=-Wl,-z,now,-z,relro,-z,max-page-size=0x1000,-z,separate-code -Clink-arg=-Wno-error -Clink-arg=-mprefer-vector-width=256 -Clink-arg=-falign-functions=32 -Clink-arg=-fno-semantic-interposition -Clink-arg=-Wl,-Bsymbolic-functions -Cprefer-dynamic=no -Zplt=off $PGO_USE_RUST"
export CARGO_HOST_RUSTFLAGS_USE="-Clinker-plugin-lto=on -Clto=fat --emit=dep-info,link,llvm-ir -Csave-temps=yes -Zunstable-options -Zcombine-cgu=yes -Ccodegen-units=1 -Clinker=clang -Ctarget-feature=+crt-static -Cembed-bitcode=yes -Cpanic=abort -Clink-arg=-fuse-ld=lld -Clink-arg=-flto -Crelocation-model=static -Cincremental=false -Cforce-frame-pointers=off -Cforce-unwind-tables=off -Cpanic=abort -Clinker=clang -Clink-arg=-Wno-unused-command-line-argument -Clink-arg=-Wall -Clink-arg=-flto -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-O2 -Clink-arg=-O3 -Clink-arg=-mno-vzeroupper -Clink-arg=-march=native -Clink-arg=-mtune=native -Clink-arg=-fuse-ld=lld -Clink-arg=-Wl,--as-needed -Clink-arg=-Wl,--build-id=sha1 -Clink-arg=-Wl,--enable-new-dtags -Clink-arg=-Wl,-mllvm,-x86-use-vzeroupper=0 -Clink-arg=-Wl,-z,now,-z,relro,-z,max-page-size=0x1000,-z,separate-code -Clink-arg=-Wno-error -Clink-arg=-mprefer-vector-width=256 -Clink-arg=-falign-functions=32 -Clink-arg=-fno-semantic-interposition -Clink-arg=-Wl,-Bsymbolic-functions -Cprefer-dynamic=no -Zplt=off $PGO_USE_RUST"

clean() {
  cargo clean || :
  rm out/bin/rsmain || :
  rm ./rsmain || :
  rm ./*.rmeta || :
  rm ./*.o || :
  rm ./*.bc || :
  rm ./*.i || :
  rm -rf false/ || :
  rm ./libinteresting.rlib || :
  rm ./libxyz.a || :
  rm ./clib.o || :
  rm /var/tmp/pgo/rustprofile.profdata || :
  rm /var/tmp/pgo/*.profdata || :
  rm /var/tmp/pgo/*.profraw || :
}

first() {
  set -x
  export CFLAGS="${CFLAGS_GENERATE}"
  export CXXFLAGS="${CXXFLAGS_GENERATE}"
  export LDFLAGS="${LDFLAGS_GENERATE}"
  export CARGO_TARGET_X86_64_UNKNOWN_LINUX_GNU_RUSTFLAGS="${CARGO_TARGET_X86_64_UNKNOWN_LINUX_GNU_RUSTFLAGS_GENERATE}"
  export RUSTFLAGS="${RUSTFLAGS_GENERATE}"
  export CARGO_HOST_RUSTFLAGS="${CARGO_HOST_RUSTFLAGS_GENERATE}"
  cargo clean || :
  rm out/bin/rsmain || :
  rm ./rsmain || :
  rm ./*.rmeta || :
  rm ./*.o || :
  rm -rf false/ || :
  rm ./libinteresting.rlib || :
  rm libxyz.a || :
  rm clib.o || :
  rm /var/tmp/pgo/rustprofile.profdata || :
  rm /var/tmp/pgo/*.profdata || :
  rm /var/tmp/pgo/*.profraw || :
  /usr/bin/clang ./clib.c $CFLAGS -c -o ./clib.o -v
  /usr/bin/llvm-ar crus ./libxyz.a ./clib.o
  mkdir -p target/x86_64-unknown-linux-gnu/release/deps/
  cp -f ./libxyz.a target/x86_64-unknown-linux-gnu/release/deps/
  cargo install -Zunstable-options -Zhost-config -Ztarget-applies-to-host --jobs 16 -vv --offline --locked --no-track --force --profile release --target x86_64-unknown-linux-gnu --path . --root out/ --all-features
#   cargo rustc -Zunstable-options -Zhost-config -Ztarget-applies-to-host --jobs 16 -vv --offline --locked --profile release --target x86_64-unknown-linux-gnu -- --emit llvm-bc
#     rustc --verbose $RUSTFLAGS_GENERATE -L. ./interesting.rs
# #     rustc --verbose $RUSTFLAGS_GENERATE -L. --emit=llvm-ir -o ./rsmain1.ll ./main.rs
#     rustc --verbose $RUSTFLAGS_GENERATE -L. -o ./rsmain ./main.rs
  out/bin/rsmain
  exa --long --all --icons out/bin/rsmain
  ldd out/bin/rsmain
  set +x
}

second() {
  set -x
  export CFLAGS="${CFLAGS_USE}"
  export CXXFLAGS="${CXXFLAGS_USE}"
  export LDFLAGS="${LDFLAGS_USE}"
  export CARGO_TARGET_X86_64_UNKNOWN_LINUX_GNU_RUSTFLAGS="${CARGO_TARGET_X86_64_UNKNOWN_LINUX_GNU_RUSTFLAGS_USE}"
  export RUSTFLAGS="${RUSTFLAGS_USE}"
  export CARGO_HOST_RUSTFLAGS="${CARGO_HOST_RUSTFLAGS_USE}"
  cargo clean || :
  rm out/bin/rsmain || :
  rm ./rsmain || :
  rm ./*.rmeta || :
  rm ./*.o || :
  rm -rf false/ || :
  rm ./libinteresting.rlib || :
  rm libxyz.a || :
  rm clib.o || :
#   /usr/bin/llvm-profdata merge -o /var/tmp/pgo/rustprofile.profdata /var/tmp/pgo/*.profraw || :
#   /usr/lib64/rustlib/x86_64-unknown-linux-gnu/bin/llvm-profdata merge -o /var/tmp/pgo/rustprofile.profdata /var/tmp/pgo/*.profraw || :
  /usr/bin/llvm-profdata merge -output=/var/tmp/pgo/code.profdata /var/tmp/pgo/ || :
#   /usr/lib64/rustlib/x86_64-unknown-linux-gnu/bin/llvm-profdata merge -output=/var/tmp/pgo/code.profdata /var/tmp/pgo/ || :
  /usr/bin/clang ./clib.c $CFLAGS -c -o ./clib.o -v
  /usr/bin/llvm-ar crus ./libxyz.a ./clib.o
  mkdir -p target/x86_64-unknown-linux-gnu/release/deps/
  cp -f ./libxyz.a target/x86_64-unknown-linux-gnu/release/deps/
  cargo install -Zunstable-options -Zhost-config -Ztarget-applies-to-host --jobs 16 -vv --offline --locked --no-track --force --profile release --target x86_64-unknown-linux-gnu --path . --root out/ --all-features
#   rustc --verbose $RUSTFLAGS_USE -L. ./interesting.rs
# #     rustc --verbose $RUSTFLAGS_USE -L. --emit=llvm-ir -o ./rsmain2.ll ./main.rs
#   rustc --verbose $RUSTFLAGS_USE -L. -o ./rsmain ./main.rs
  out/bin/rsmain
  exa --long --all --icons out/bin/rsmain
  ldd out/bin/rsmain
#   cargo rustc -Zunstable-options -Zhost-config -Ztarget-applies-to-host --jobs 16 -vv --offline --locked --profile release --target x86_64-unknown-linux-gnu --all-features -- --emit=llvm-bc
  set +x
}

case $1 in
  c)
    clean;
    ;;
  1)
    first;
    ;;
  2)
    second;
    ;;
  *)
    echo "Unknown option"
    ;;
esac
